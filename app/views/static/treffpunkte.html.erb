    <%= link_to "Petaluma Oktoberfest", "http://odhslodges.org/petaluma/oktoberfest/", target: "_blank" %>
    </p>
    </li>
  </ul>
  <% @meetings.each do |meeting| %>
    <ul class="list-group">
      <li class="list-group-item">
        <h3><%= meeting[:name] %></h3>
        <address>
          <strong><%= meeting[:place] %></strong><br>
          <%= meeting[:street] %><br>
          <%= meeting[:city] %>, <%= meeting[:state] %>. <%= meeting[:zip] %> <br> 
        </address>
        <time>
          <%= meeting[:start_time] %><br>
          <%= simple_format meeting[:normal_day] %><br>
          <% if meeting[:exception_day] != "" %>
            <%= meeting[:exception_day] %><br>
          <% end %>
        </time>
        <%= link_to(meeting[:website],meeting[:website])    %>
      </li>
    </ul>
  <% end %>
